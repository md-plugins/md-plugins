{
  "name": "mdplugins_docs",
  "version": "0.1.0-alpha.25",
  "description": "Docs site for @md-plugins, the home of Q-Press!",
  "productName": "@md-plugins",
  "author": "Jeff <galbraith64@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar prepare && quasar build",
    "build:ssr": "quasar prepare && quasar build -m ssr",
    "start:ssr": "cd dist/ssr && node index.js",
    "prepared": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.16",
    "pinia": "^2.3.1",
    "prismjs": "^1.29.0",
    "quasar": "^2.17.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@md-plugins/md-plugin-headers": "workspace:*",
    "@md-plugins/shared": "workspace:*",
    "@md-plugins/vite-examples-plugin": "workspace:*",
    "@md-plugins/vite-md-plugin": "workspace:*",
    "@quasar/app-vite": "^2.0.8",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^22.13.1",
    "@types/prismjs": "^1.26.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "markdown-it-container": "^4.0.0",
    "prettier": "^3.5.0",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.8.0",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
